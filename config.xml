<gap:splash src="images/splash/ios/Default-Portrait-736h@3x.png" gap:platform="ios" width="1242" height="2208" />
<gap:splash src="images/splash/ios/Default-Landscape-736h@3x.png" gap:platform="ios" width="2208" height="1242" />

<!-- iPad -->
<gap:splash src="images/splash/ios/Default-Portrait.png" gap:platform="ios" width="768" height="1024" />
<gap:splash src="images/splash/ios/Default-Landscape.png" gap:platform="ios" width="1024" height="768" />

<!-- Retina iPad -->
<gap:splash src="images/splash/ios/Default-Portrait@2x.png" gap:platform="ios" width="1536" height="2048" />
<gap:splash src="images/splash/ios/Default-Landscape@2x.png" gap:platform="ios" width="2048" height="1536" />

		
		<access origin="*.urbisclub.com" />
		<access origin="http://www.esteticaclub.com" />
		<access origin="*" />
		<access origin="*.google.com" />
		<access origin="*.googleapis.com" />
		<access origin="*.gstatic.com" />
		<access origin="*.twitter.com" />
		<access origin="*.twitter.es" />
		
		<gap:plugin name="com.phonegap.plugins.childbrowser" />
<!--CALENDARIO-->
		<gap:plugin name="nl.x-services.plugins.calendar" />
<feature name="Calendar">
	<!-- for iOS -->
	<param name="ios-package" value="Calendar" />
	<!-- for Android -->
	<param name="android-package" value="nl.xservices.plugins.Calendar" />
</feature>
	</widget>