<?xml version="1.0" encoding="UTF-8"?>
	<widget xmlns = "http://www.w3.org/ns/widgets"
		xmlns:gap = "http://phonegap.com/ns/1.0"
		id        = "com.esteticaclientes"
		versionCode="1.0"
		version   = "1.0.0">
		<name>Estética Clientes</name>
		<description>Club para centros de estética</description>
		<author href="http://www.esteticaclub.com" email="info@casualiswebs.com">
Casualis Webs
		</author>
		<preference name="phonegap-version" value="3.6.3" />
		<preference name="orientation" value="portrait" />
		
		<!--Retirar barra de arriba en IOS7-->
		<gap:config-file platform="ios" parent="UIViewControllerBasedStatusBarAppearance">
		<false/>
		</gap:config-file>
		<feature name="StatusBar">
				<param name="ios-package" value="CDVStatusBar" onload="false" />
		</feature>
		
		<preference name="fullscreen" value="true" />
		<preference name="target-device" value="universal" />
		<preference name="webviewbounce" value="true" />
		<preference name="prerendered-icon" value="true" />
		<preference name="stay-in-webview" value="false" />
		<preference name="ios-statusbarstyle" value="default" />
		<preference name="detect-data-types" value="true" />
		<preference name="exit-on-suspend" value="false" />
		<preference name="show-splash-screen-spinner" value="true" />
		<preference name="auto-hide-splash-screen" value="true" />
		<preference name="EnableViewportScale" value="false" />
		<preference name="MediaPlaybackRequiresUserAction" value="false" />
		<preference name="AllowInlineMediaPlayback" value="false" />
		<preference name="BackupWebStorage" value="cloud" />
		<preference name="TopActivityIndicator" value="gray" />
		<preference name="KeyboardDisplayRequiresUserAction" value="true" />
		<preference name="HideKeyboardFormAccessoryBar" value="false" />
		<preference name="SuppressesIncrementalRendering" value="false" />
		<preference name="android-minSdkVersion" value="7" />
		<preference name="android-installLocation" value="auto" />
		<preference name="SplashScreenDelay" value="10000" />
		<preference name="ErrorUrl" value=""/>
		<preference name="BackgroundColor" value=""/>
		<preference name="KeepRunning" value="true"/>
		<preference name="DisallowOverscroll" value="true"/>
		<preference name="LoadingDialog" value=","/> 
		<preference name="LoadUrlTimeoutValue" value="50000" />
		<preference name="disable-cursor" value="true" />
		<gap:platform name="ios" />
		<!--<gap:platform name="android" />-->
		<feature name="http://api.phonegap.com/1.0/geolocation"/>
		<feature name="http://api.phonegap.com/1.0/network"/>
		<feature name="http://api.phonegap.com/1.0/notification"/>
		<gap:plugin name="org.apache.cordova.device" version="0.2.8" />
		<gap:plugin name="org.apache.cordova.device-motion"  />
		<gap:plugin name="org.apache.cordova.device-orientation" />
		<gap:plugin name="org.apache.cordova.dialogs"  />
		<gap:plugin name="org.apache.cordova.file"  />
		<gap:plugin name="org.apache.cordova.geolocation"  />
		<gap:plugin name="org.apache.cordova.inappbrowser"  />
		<gap:plugin name="org.apache.cordova.network-information"  />
		<gap:plugin name="org.apache.cordova.splashscreen"  />
		<gap:plugin name="org.apache.cordova.vibration"  />
		<gap:plugin name="de.appplant.cordova.plugin.local-notification" />
		
		


<!-- iPhone 6 / 6+ -->
<icon src="images/iconos/ios/icon-60@3x.png" gap:platform="ios" width="180" height="180" />

<!-- iPhone / iPod Touch  -->
<icon src="images/iconos/ios/icon-60.png" gap:platform="ios" width="60" height="60" />
<icon src="images/iconos/ios/icon-60@2x.png" gap:platform="ios" width="120" height="120" />

<!-- iPad -->
<icon src="images/iconos/ios/icon-76.png" gap:platform="ios" width="76" height="76" />
<icon src="images/iconos/ios/icon-76@2x.png" gap:platform="ios" width="152" height="152" />

<!-- Settings Icon -->
<icon src="images/iconos/ios/icon-small.png" gap:platform="ios" width="29" height="29" />
<icon src="images/iconos/ios/icon-small@2x.png" gap:platform="ios" width="58" height="58" />

<!-- Spotlight Icon -->
<icon src="images/iconos/ios/icon-40.png" gap:platform="ios" width="40" height="40" />
<icon src="images/iconos/ios/icon-40@2x.png" gap:platform="ios" width="80" height="80" />

<!-- iPhone / iPod Touch -->
<icon src="images/iconos/ios/icon.png" gap:platform="ios" width="57" height="57" />
<icon src="images/iconos/ios/icon@2x.png" gap:platform="ios" width="114" height="114" />

<!-- iPad -->
<icon src="images/iconos/ios/icon-72.png" gap:platform="ios" width="72" height="72" />
<icon src="images/iconos/ios/icon-72@2x.png" gap:platform="ios" width="144" height="144" />

		
		<!-- iPhone and iPod touch -->
<gap:splash src="images/splash/ios/Default.png" gap:platform="ios" width="320" height="480" />
<gap:splash src="images/splash/ios/Default@2x.png" gap:platform="ios" width="640" height="960" />

<!-- iPhone 5 / iPod Touch (5th Generation) -->
